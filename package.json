{
  "name": "elm-pages-example",
  "version": "1.0.0",
  "description": "Example site built with elm-pages.",
  "scripts": {
    "start": "npm run generate-all &&  elm-pages dev",
    "serve": "npm run generate-all && elm-pages build && http-server ./dist -a localhost -p 3000 -c-1",
    "generate-all": "npm run generate:tailwind",
    "build": "NODE_ENV=production npm run generate-all && npm install && elm-pages build --keep-cache",
    "generate:tailwind": "postcss tailwind.css -o static/tailwind-compiled.css"
  },
  "author": "Dillon Kearns",
  "license": "BSD-3",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@tailwindcss/ui": "^0.1.3",
    "cssnano": "^4.1.10",
    "elm-optimize-level-2": "^0.3.5",
    "highlight.js": "^9.15.10",
    "node-sass": "^7.0.0",
    "tailwindcss": "^1.2.0",
    "terser": "^5.7.1"
  },
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^4.2.2",
    "elm-pages": "file:./elm-pages",
    "elm-review": "^2.7.0",
    "http-server": "^14.0.0",
    "postcss-cli": "^7.1.0"
  }
}
